#include <iostream>
using namespace std;

class A
{
 private:
     int a;
 public:
    void set_data(int n)
    {
    a = n;
    }
    int get_data()
    {
     return a + 1;
    }
    
};

int main() {

A obj[50];

int n,i;
cin >> n;

for(i = 0; i < n; i++)
{
obj[i].set_data(i); 
}

for(i = 0; i < n; i++)
{
cout << obj[i].get_data() << endl;
}


    return 0;
}
