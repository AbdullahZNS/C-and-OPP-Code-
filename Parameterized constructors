#include <iostream>
using namespace std;

class rectangle{
  
  private:
       int height,width;
  
  public:
  
        rectangle(int a, int b) //Parameterized Constructor
       {
       height = a;
       width  = b;
       }
       
       int area()
       {
        return height * width;
       }
    
};



int main()
{
 rectangle obj(5,6);    
 cout << "The area of the rectangle is: " << obj.area() << endl;
    
    
    return 0;
}
