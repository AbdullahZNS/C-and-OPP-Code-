/* Constructor in C++ */

#include <iostream>
using namespace std;

class Constructor
{
  private:
          int x,y;
  
  public:
         Constructor();
       
};

Constructor :: Constructor()
{
cout << "Value of x and y" << endl;
          cin >> x >> y;
          
           cout << "x = " << x << endl;
           cout << "y = " << y << endl;
          
}


int main()
{
 Constructor C1;
 
 
   
    return 0;
}
